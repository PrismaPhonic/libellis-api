{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "_token"
  ],
  "properties": {
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "Best Hip Hop Artist of 2019"
      ],
      "pattern": "^(.*)$"
    },
    "content_type": {
      "$id": "#/properties/content_type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "ranked"
      ],
      "pattern": "^(ranked|multiple)$"
    },
    "_token": {
      "$id": "#/properties/_token",
      "type": "string",
      "title": "The Token Schema",
      "default": "",
      "examples": [
        "23BFfdfldskjkei@#$#kjsdffksld"
      ],
      "pattern": "^(.*)$"
    }
  },
  "additionalProperties": false
}
